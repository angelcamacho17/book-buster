{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "data-store": [
        "dist/data-store/data-store",
        "dist/data-store"
      ],
      "material-lib": [
        "dist/material-lib/material-lib",
        "dist/material-lib"
      ],
      "models-lib": [
        "dist/models-lib/models-lib",
        "dist/models-lib"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}